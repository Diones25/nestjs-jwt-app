{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage",
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/src/app/users/users.module.ts" 
  ]
}
